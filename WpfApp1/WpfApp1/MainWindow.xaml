<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="195*"/>
            <RowDefinition Height="22*"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="1" x:Name="back" Width="100" HorizontalAlignment="Right" Background="DarkKhaki" Click="back_Click">назад</Button>
        <Button Margin="595,390,0,0" x:Name="vpered" Width="100" Background="DarkKhaki" HorizontalAlignment="Left"  Height="44" Grid.RowSpan="2" VerticalAlignment="Top" Click="vpered_Click">вперед</Button>
        <Frame x:Name="fram" Grid.Row="0" NavigationUIVisibility="Hidden">
        </Frame>
    </Grid>
</Window>
